&pcie{
	pinctrl-names = "default";
	/* pinctrl-0 = <&pinctrl_pcie>; */

	/* disable-gpio = <&pca6416_6_20 10 GPIO_ACTIVE_LOW>; */
	reset-gpio = <&pca6416_6_21 11 GPIO_ACTIVE_LOW>;
